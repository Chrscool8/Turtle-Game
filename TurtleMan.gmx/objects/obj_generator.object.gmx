{
    instance_create(i*tile_size, -tile_size*2, obj_water_new);
    instance_create(i*tile_size, -tile_size*4, obj_water_deep_new);
    instance_create(i*tile_size, -tile_size*6, obj_water_deep_new);
    instance_create(i*tile_size, -tile_size*8, obj_water_deep_new);
    instance_create(i*tile_size, -tile_size*10, obj_water_deep_new);
}

// decorations
repeat(map_width*map_height/50)
{
    instance_create(floor(random(map_width*tile_size)), floor(random(map_height*tile_size)), obj_deco);
}

repeat(3)
